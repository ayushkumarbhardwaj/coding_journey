Frequencies in a Limited Array

class Solution {
  public:
    // Function to count the frequency of all elements from 1 to N in the array.
    vector<int> frequencyCount(vector<int>& arr) {
        // code here
        unordered_map<int,int>m;
        
        for(int x : arr) {
            m[x]++;
        }
        for(int i=1;i<=arr.size();i++)
        {
            arr[i-1]=m[i];
        }
        return arr;
    }
};
