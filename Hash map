Subarray Sum Equals K

class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int>m;
        int maxLen=0;
        int sum=0;
        m[0]=1;
        for(int i:nums)
        {
          sum+=i;
        
          if(m.find(sum-k)!=m.end())  maxLen+=m[sum-k];
    
            m[sum]++;
        }
        return maxLen;
    }
};





<<<<<<<<<<<<<<<<<<<<...............>>>>>>>>>>>>>>>>>>>>
Longest Subarray with Sum K

class Solution {
  public:
    int longestSubarray(vector<int>& arr, int k) {
        // code here
        int maxLen=0,sum=0;
        unordered_map<int ,int>m;
        for(int i=0;i<arr.size();i++)
        {
            sum+=arr[i];
            if(sum==k)
            {
                maxLen=max(maxLen,i+1);
            }
            
            int req=sum-k;
            if(m.find(req)!=m.end())
            {
                int len=i-m[req];
                maxLen=max(maxLen,len);
            }
            if(m.find(sum)==m.end())
            {
                m[sum]=i;
            }
        }
        return maxLen;
    }
};








<<<<<<<<<<<<<<<...................>>>>>>>>>>>>>>>>
Find Players With Zero or One Losses

class Solution {
public:
    vector<vector<int>> findWinners(vector<vector<int>>& matches) {
        unordered_map<int,int>lose;
        for(int i=0;i<matches.size();i++)
        {
            int loss=matches[i][1];
            lose[loss]++;
        }
        vector<int>alose,blose;
        unordered_set<int>seenA,seenB;

        for(int i=0;i<matches.size();i++)
        {
            int frist=matches[i][0];
            int second=matches[i][1];
            if(lose.find(frist)==lose.end() && !seenA.count(frist))
            {
                alose.push_back(frist);
                seenA.insert(frist);
            }
            if(lose[second]==1 && !seenB.count(second))
            {
                blose.push_back(second);
                seenB.insert(second);
                
            }
        }
        sort(alose.begin(),alose.end());
        sort(blose.begin(),blose.end());
        return {alose,blose};
    }
};









<<<<<<<<<<<<<<<<<...............>>>>>>>>>>>>>>>
Count Elements With Maximum Frequency

class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        int ans=0;
        unordered_map<int,int>m;
        for(int i:nums)
        {
            m[i]++;
        }
        for(auto j:m)
        {
           ans=max(ans,j.second);
        }
        int count=0;
        for(auto k:m)
        {
            if(ans==k.second)
            {
                count+=k.second;
            }
        }
        return count;
    }
};






<<<<<<<<<<<<<<<<..............>>>>>>>>>>>>>>>>
Frequencies in a Limited Array

class Solution {
  public:
    // Function to count the frequency of all elements from 1 to N in the array.
    vector<int> frequencyCount(vector<int>& arr) {
        // code here
        unordered_map<int,int>m;
        
        for(int x : arr) {
            m[x]++;
        }
        for(int i=1;i<=arr.size();i++)
        {
            arr[i-1]=m[i];
        }
        return arr;
    }
};
