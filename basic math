Vowel or Not


class Solution {
  public:
    string isVowel(char c) {
        // code here
        if(c=='a' || c=='e' || c=='i' ||c=='o' || c=='u' || c=='A' || c=='E' || c=='I' || c=='O' || c=='U')
        {
            return "YES";
        }
        else
        {
            return "NO";
        }
    }
};




>>>>>>>>>>>>>>>>>>>>...........................<<<<<<<<<<<<<<<<<<<<<<<
Repeated sum of digits

class Solution {
  public:
    int repeatedSumOfDigits(int N) {
        // code here
     if(N<10)
     {
         return N;
     }else {
     int sum=0;
     while(N!=0)
     {
         int b=N%10;
         sum=sum+b;
         N=N/10;
     }N=sum;
     if(N<10)
     {
         return N;
     }
     return repeatedSumOfDigits( N);
     }
    }
};


>>>>>>>>>>>............<<<<<<<<<<<<<
Find n-th term of series 1, 3, 6, 10, 15, 21
class Solution {
  public:
    int findNthTerm(int n) {
        // code here
        int nth ;
        nth= n*(n+1)/2;
        return nth;
    }
};



>>>>>>>>>>>............<<<<<<<<<<<<<
Given the three sides of a triangle a, b, and c. Check whether the triangle is valid or not.

// User function Template for C++
class Solution {
  public:
    bool checkValidity(int a, int b, int c) {
        // code here
        if((a+b)>c && (b+c)>a && (c+a)>b)
        {return true;}
       return false;
    }
};



>>>>>>>>>>>............<<<<<<<<<<<<<
Series AP

// User function template for C++

class Solution {
  public:
    int nthTermOfAP(int a1, int a2, int n) {
        // code here
        int nth,d;
        d=a2-a1;
        nth=a1+(n-1)*d;
        return nth;
    }
};


