#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> a(n);
        for (int i = 0; i < n; i++)
            cin >> a[i];

        // If array already sorted â†’ YES
        if (is_sorted(a.begin(), a.end())) {
            cout << "YES\n";
            continue;
        }

        // Otherwise, check if the smallest element is at index 0
        int mn = *min_element(a.begin(), a.end());

        if (a[0] == mn)
            cout << "YES\n";
        else
            cout << "NO\n";
    }

    return 0;
}















#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        if (n % 3 == 0)
            cout << "Second\n";
        else
            cout << "First\n";
    }
    return 0;
}












