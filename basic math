 Enter your number for Reverse :123
Reverse is :321  
class Solution {
public:
    int reverse(int x) {
        long long rev =0;
        while(x!=0)
        {
           long long b=x%10;
            rev=rev*10+b;
            x=x/10;
            if (rev > INT_MAX || rev < INT_MIN) {
                return 0;
            }
        }
        return (int)rev;
    }
};
