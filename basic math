\\Frist digit of a number

int firstDigit(int n) {
    // code here
    if(n>=10)
    {
        while(n>=0)
    {
        n=n/10;
        if(n<10)
        {
           return n;
           break;
        }
        
    }
    }
    else{
        return n;
    }
    
}
















