Check Prime

#include <iostream>
using namespace std;

int main() {
    // code here
    int n;
    cin>>n;
    int count=0;
    for(int i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            count++;
            break;
        }
    }
    if(count>0)
    {
        cout<<"False";
    } else{
    cout<<"True";
    }
    return 0;
}











