#include<iostream>
#include<array>
#include<vector>
using namespace std;
int main()
{  

    
Size :3
Capacity: 4
Frist: 10
Last: 87
Element at index 1 is :65
Before pop:10 65 87
After pop: 1065
After clear Data

     vector<int> v;
     v.push_back(10);
     v.push_back(65);
     v.push_back(87);
     cout<<"Vector element is : ";
     for(int x:v)
     {
        cout<<x<<" ";
     }cout<<endl;
     cout<<"Size :"<<v.size()<<endl;
     cout<<"Capacity: "<<v.capacity()<<endl;
     cout<<"Frist: "<<v.front()<<endl;
     cout<<"Last: "<<v.back()<<endl;
     cout<<"Element at index 1 is :"<<v.at(1)<<endl;
     cout<<"Before pop:";
     for(int x :v)
     {
        cout<<x<<" ";
     }cout<<endl;
     v.pop_back();
     cout<<"After pop: ";
     for(int x:v)
     {
        cout<<x;
     }cout<<endl;
     v.clear();
     cout<<"After clear Data ";







    array<int , 5> arr={10,45,85,75,48};
    cout<<"Size of: "<<arr.size()<<endl;
    cout<<"Frist element is: "<<arr.front()<<endl;
    cout<<"Last element is: "<<arr.back()<<endl;
    cout<<"Element at index 3: "<<arr.at(3)<<endl;
    cout<<"All elements : ";
    for(int x: arr)
    {
        cout<<x<<" ";
    }
    cout<<endl;
    arr.fill(8);
    cout<<"After fill :";
    for(int x:arr)
    {
        cout<<x<<" ";
    }
Size of: 5
Frist element is: 10
Last element is: 48
Element at index 3: 75
All elements : 10 45 85 75 48
After fill :8 8 8 8 8



    return 0;
}
