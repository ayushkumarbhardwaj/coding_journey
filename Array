Number of occurrence

class Solution {
  public:
    int countFreq(vector<int>& arr, int target) {
        // code here
        int ans=0;
        int n=arr.size();
        for(int i = 0; i < n; i++)
        {
            if(arr[i]==target)
            {
                ans++;
            }
        }
        return ans;
    }
};









<<<<<<<<<<<<<<<<<...............>>>>>>>>>>>>>>
Find Numbers with Even Number of Digits

class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int count =0,n=nums.size();
        for(int i=0;i<n;i++)
        {
            int a=0;
            while(nums[i]>0)
            {
                nums[i]=nums[i]/10;
                a++;
            }
            if(a%2==0)
            {
                count++;
            }
            a=0;
        }
        return count;
    }
};









<<<<<<<<<<<<<<<<<<<................>>>>>>>>>>>>>>>>>>
Container With Most Water

class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size()-1;
        int i=0;
        int ans=0;
        int maxw=0;
        while(n>i)
        {
          if(height[i]<=height[n])
          {
            ans=(n-i)*height[i];
            i++;
            maxw=max(ans,maxw);
          }
          else
          {
            ans=(n-i)*height[n];
            n--;
           maxw=max(ans,maxw);
          }
        }
        return maxw;
    }
};






<<<<<<<<<<<<<<<<<<<..............>>>>>>>>>>>>>>>>>
Reverse an Array

class Solution {
  public:
    void reverseArray(vector<int> &arr) {
        // code here
        int n=arr.size()-1;
        int i=0;
        while(i<n)
        {
            swap(arr[i],arr[n]);
            i++;
            n--;
        }
    }
};










<<<<<<<<<<<<<<<<<<<<<...................>>>>>>>>>>>>>>>>>>>>
Majority Element II

class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int n=nums.size();
        vector<int>arr;
        int low=0,i=0,count=0;
        while(n>i)
        {
            if(nums[low]==nums[i])
            {
               count++;
               if(count>(n/3))
               {
                if(arr.size()==0)
                arr.push_back(nums[i]);
                if(arr.back()!=nums[i])
                arr.push_back(nums[i]);
               }
               i++;
            }
            else
            {
                low++;
                count=0;
            }
        }
        return arr;
    }
};







<<<<<<<<<<<<<<<<<<<<<...............>>>>>>>>>>>>>>>>>>>>>
Dutch National Flag Algorithm
Sort Colors

class Solution {
public:
    void sortColors(vector<int>& nums) {
        int n=nums.size();
        int low=0,mid=0,high=n-1;
        while(mid<=high)
        {
           if(nums[mid]==0)
           {
            swap(nums[mid],nums[low]);
            low++,mid++;
           }
           else if(nums[mid]==1)
           {
            mid++;
           }
           else if(nums[mid]==2)
           {
            swap(nums[mid],nums[high]);
            high --;
           }
           
        }
    }
};









<<<<<<<<<<<<<<<<.................>>>>>>>>>>>>>>>>>
Merge Sorted Array

class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        vector<int>arr;
       for(int i=0;i<m;i++)
       {
        arr.push_back(nums1[i]);
       }
       for(int i=0;i<n;i++)
       {
        arr.push_back(nums2[i]);
       }
        nums1=arr;
        sort(nums1.begin(),nums1.end());
    }
};









<<<<<<<<<<<<<<<<<<<<..................>>>>>>>>>>>>>>>>>>>>
Median of Two Sorted Arrays

class Solution {
public:
    double findMedianSortedArrays(vector<int>& nums1, vector<int>& nums2) {
      nums1.insert(nums1.end(),nums2.begin(),nums2.end());
      sort(nums1.begin(),nums1.end());
      int N=nums1.size();


        
        double ans=0;
       if((N)%2!=0)
       {
           int a=(N-1)/2;
           ans=nums1[a];
           return ans;
       }
       else if(N%2==0)
       {
           int a=(N)/2;
           ans=(nums1[a]+nums1[a-1])/2.0;
           return ans;
       }
       return 0;
    }
};








<<<<<<<<<<<<<<<<<<<<<.............>>>>>>>>>>>>>>>>>>>>>
Plus One

class Solution {
public:
    vector<int> plusOne(vector<int>& digits) {
        int n=digits.size();
        for(int i=n-1;i>=0;i--)
        {
            if(digits[i]<9)
            {
                digits[i]++;
                return digits;
            }
            digits[i]=0;

        }
           digits.insert(digits.begin(),1);
        return digits;
    }
};











<<<<<<<<<<<<<<<<..............>>>>>>>>>>>>>>>>>>>>
Single Number

class Solution {
public:
    int singleNumber(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int n=nums.size();
        
        for(int i=0;i<n-1;i+=2)
        {
           if(nums[i]!= nums[i+1])
           {
            return nums[i];
           }
        }
        return nums[n-1];
    }
};












<<<<<<<<<<<<<<<<<<<<<<<<<................>>>>>>>>>>>>>>>>>>>>>>
Maximum Consecutive Ones

class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n=nums.size();
        int count=0;
        int a=0;
        for(int i=0;i<n;i++)
        {
           if(nums[i]==1)
           {
             count++;
           }
           else
           {
             if(count>a)
             {
                a=count;
             }
             count=0;
           }

        }
        if(count>a)
        {
            return count;
        }
        else
        {
            return a;
        }
    }
};










<<<<<<<<<<<<<<<<<<<<<<<<<.................>>>>>>>>>>>>>>>>>>
Intersection of Two Sorted Arrays

class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        int n=nums1.size();
        int m=nums2.size();
        sort(nums1.begin(),nums1.end());
        sort(nums2.begin(),nums2.end());
        vector<int>arr;
        int i=0;
        int j=0;

        while(i<n && j<m)
        {
            if(nums1[i]>nums2[j])
            {
                j++;
            }
            else if(nums1[i]<nums2[j])
            {
                i++;
            }
            else 
            {
                if(arr.empty() || arr.back() != nums1[i])
                {
                    arr.push_back(nums1[i]);
                }
                i++;
                j++;
            }
        }
        
        

        return arr;
    }
};














<<<<<<<<<<<<<<<<<<<<<<<<<<<.................>>>>>>>>>>>>>>>>>>>>>>>>>>>
Union of 2 Sorted Arrays

class Solution {
  public:
    vector<int> findUnion(vector<int> &a, vector<int> &b) {
        // code here
        vector<int>arr;
        int n=a.size();
        int m=b.size();
        
        arr=a;
        for(int i=0;i<m;i++)
        {
            arr.push_back(b[i]);
        }
        sort(arr.begin(),arr.end());
        vector<int>p;
        p.push_back(arr[0]);
        for(int j=1;j<arr.size();j++)
        {
            if(arr[j]!=arr[j-1])
            {
                p.push_back(arr[j]);
            }
        }
        return p;
    }
};

####### Optimal Solution


class Solution {
  public:
    vector<int> findUnion(vector<int> &a, vector<int> &b) {
        vector<int> arr;
        int n = a.size(), m = b.size();
        int i = 0, j = 0;

        while(i < n && j < m) {
            if(a[i] > b[j]) {
                if(arr.empty() || arr.back() != b[j])
                    arr.push_back(b[j]);
                j++;   
            }
            else if(a[i] < b[j]) {
                if(arr.empty() || arr.back() != a[i])
                    arr.push_back(a[i]);
                i++;   
            }
            else {
                if(arr.empty() || arr.back() != a[i])
                    arr.push_back(a[i]);
                i++;
                j++;   
            }
        }

        while(i < n) {
            if(arr.empty() || arr.back() != a[i])
                arr.push_back(a[i]);
            i++;
        }

        while(j < m) {
            if(arr.empty() || arr.back() != b[j])
                arr.push_back(b[j]);
            j++;
        }

        return arr;
    }
};







<<<<<<<<<<<<<<<<<<<<<<<<<<<.....................>>>>>>>>>>>>>>>>>>>>>>>>>>
Maximum Repited Element

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count = 0;
        int candidate = 0;

        for (int num : nums) {
            if (count == 0) {
                candidate = num;
            }

            if (num == candidate)
                count++;
            else
                count--;
        }
        return candidate;
    }
};











<<<<<<<<<<<<<<<<<<<<<<<<...................>>>>>>>>>>>>>>>>>>>>>
Best Time to Buy and Sell Stock

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int buy = prices[0];
        int profit = 0;

        for(int i = 1; i < prices.size(); i++) {
            if(prices[i] < buy)
                buy = prices[i];
            profit = max(profit, prices[i] - buy);
        }
        return profit;
    }
};








<<<<<<<<<<<<<<<<<<<<<<<.....................>>>>>>>>>>>>>>>>>>>>>>
Frist Repeated Element Index

class Solution {
  public:
    int firstRepeated(vector<int> &arr) {
        // code here
        int n=arr.size();
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if(arr[i]==arr[j])
                {
                    return i+1;
                }
            }
        }
        return -1;
    }
};







<<<<<<<<<<<<<...........>>>>>>>>>>>>>>
Duplicate Array 
class Solution {
public:
    vector<int> findDuplicates(vector<int>& arr) {
        int n = arr.size();
        vector<int> freq(n + 1, 0);
        vector<int> ans;

        for (int x : arr) {
            freq[x]++;
            if (freq[x] == 2) {
                ans.push_back(x);
            }
        }

    

        return ans;
    }
};








